  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/checkCC.js"
    },
    {
        "type": "node",
        "request": "launch",
        "name": "Launch Demo CheckCC",
        "program": "${workspaceFolder}/checkCC.js",
        "cwd": "${workspaceFolder}"
    },
    {
        "name": "Jest",
        "type": "node",
        "request": "launch",
        "env": { "NODE_ENV": "test" },
        "cwd": "${workspaceRoot}",
        "program": "${workspaceRoot}/node_modules/.bin/jest",
        "stopOnEntry": false,
        "args": [
            "-i"
        ],
        "runtimeArgs": ["--nolazy"],
        "console": "internalConsole",
        "sourceMaps": false,
        "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}